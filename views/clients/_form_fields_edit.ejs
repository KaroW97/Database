<div>
    <input type="text" name="name" placeholder="imie" value="<%=clients.name%>" required >
    <input type="text"  name="lastName" placeholder="nazwisko" value="<%=clients.lastName%>"required >
  
</div>
<div>
    <input type="tel" minlength="9" name="phoneNumber" placeholder="telefon"  value="<%=clients.phoneNumber%>" required>
    <input type="date" name="dateOfBirth" value="<%=clients.dateOfBirth == null ? '' : clients.dateOfBirth.toISOString().split('T')[0]%>"   >
    
</div>

<h4>Diagnoza skóry</h4>
          

<div class="diagnos_wrapper" >
  
    <div class="skin_diagnose_left">             
        <div class="skin_diagnos_label">
            <%if(clients.skinDiagnoseAll.drySkin.value != true){%>
                <input type="checkbox" id="dry_skin" name="drySkin"  value="<%=clients.skinDiagnoseAll.drySkin.value=true%>" >
            <%}else{%>
                <input type="checkbox" id="dry_skin" name="drySkin"  value="<%=clients.skinDiagnoseAll.drySkin.value=true%>"checked >
            <%}%>
            <span class="new_checbox"></span>
            <i for="dry_skin" class="fas fa-check"></i>
            <label for="dry_skin"> Suchość</label>
        </div>
        <div class="skin_diagnos_label">
            <%if(clients.skinDiagnoseAll.wrinkless.value != true){%>
                <input type="checkbox" id="wrinkless_skin" value="<%=clients.skinDiagnoseAll.wrinkless.value=true%>" name="wrinkless"> 
            <%}else{%>
                <input type="checkbox" id="wrinkless_skin" value="<%=clients.skinDiagnoseAll.wrinkless.value=true%>" name="wrinkless" checked> 
            <%}%>
            <span class="new_checbox"></span>
            <i for="wrinkless_skin" class="fas fa-check"></i>
            <label for="wrinkless_skin">Zmarszczki i drobne linie</label>
        </div>
        <div class="skin_diagnos_label">
            <%if(clients.skinDiagnoseAll.lackfirmnes.value != true){%>
                <input type="checkbox" id="juicy_skin" value="<%=clients.skinDiagnoseAll.lackfirmnes.value=true%>" name="lackfirmnes">
            <%}else{%>
                <input type="checkbox" id="juicy_skin" value="<%=clients.skinDiagnoseAll.lackfirmnes.value=true%>" name="lackfirmnes" checked>
            <%}%>
            <span class="new_checbox"></span>
            <i for="juicy_skin" class="fas fa-check"></i>
            <label for="juicy_skin">Brak jędrności</label>
        </div> 
        <div class="skin_diagnos_label">
            <%if(clients.skinDiagnoseAll.nonuniformColor.value != true){%>
                    <input type="checkbox" id="dif_color_skin" value="<%=clients.skinDiagnoseAll.nonuniformColor.value=true%>" name="nonuniformColor">
            <%}else{%>
                    <input type="checkbox" id="dif_color_skin" value="<%=clients.skinDiagnoseAll.nonuniformColor.value=true%>" name="nonuniformColor" checked>
             <%}%>
            <span class="new_checbox"></span>
            <i for="dif_color_skin" class="fas fa-check"></i>
            <label for="dif_color_skin">Niejednolity koloryt</label>
        </div> 
        <div  class="skin_diagnos_label">
            <%if(clients.skinDiagnoseAll.tiredness.value != true){%>
                <input type="checkbox" id="tired_skin" value="<%=clients.skinDiagnoseAll.tiredness.value=true%>" name="tiredness">
            <%}else{%>
                <input type="checkbox" id="tired_skin" value="<%=clients.skinDiagnoseAll.tiredness=true%>" name="tiredness" checked>
            <%}%>
            <span class="new_checbox"></span>
            <i for="tired_skin" class="fas fa-check"></i>
            <label for="tired_skin">Zmęczenie - stres</label>
        </div> 
        <div class="skin_diagnos_label">
            <%if(clients.skinDiagnoseAll.acne.value != true){%>
                <input type="checkbox" id="spotted_skin" value="<%=clients.skinDiagnoseAll.acne.value=true%>" name="acne">
            <%}else{%>
                <input type="checkbox" id="spotted_skin" value="<%=clients.skinDiagnoseAll.acne.value=true%>" name="acne" checked>         
            <%}%>
            <span class="new_checbox"></span>
            <i for="spotted_skin" class="fas fa-check"></i>
            <label for="spotted_skin">Trądzik grudkowy</label>
        </div>
        <div class="skin_diagnos_label">
            <%if(clients.skinDiagnoseAll.smokerSkin.value != true){%>
                <input type="checkbox" id="smoker_skin" value="<%=clients.skinDiagnoseAll.smokerSkin.value=true%>" name="smokerSkin">
            <%}else{%>
                <input type="checkbox" id="smoker_skin" value="<%=clients.skinDiagnoseAll.smokerSkin.value=true%>" name="smokerSkin" checked>
            <%}%>
            <span class="new_checbox"></span>
            <i  for="smoker_skin" class="fas fa-check"></i>
            <label for="smoker_skin">Skóra palacza</label>
        </div> 
    </div>

    <div class="skin_diagnose_right">
        <div class="skin_diagnos_label">
            <%if(clients.skinDiagnoseAll.fatSkin.value != true){%>
                <input type="checkbox" id="fat_skin" value="<%=clients.skinDiagnoseAll.fatSkin.value=true%>" name="fatSkin">
            <%}else{%>
