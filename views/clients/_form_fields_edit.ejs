            <span class="new_checbox"></span>
            <label for="capillaries_skin">Rozszerzone naczynka</label>
        </div>
        <div class="skin_diagnos_label">
            <%if(clients.skinDiagnoseAll.papularPustularAcne.value != true){%>
                <input type="checkbox" id="acne_skin" value="<%=clients.skinDiagnoseAll.papularPustularAcne.value=true%>" name="papularPustularAcne">
            <%}else{%>
                <input type="checkbox" id="acne_skin" value="<%=clients.skinDiagnoseAll.papularPustularAcne.value=true%>" name="papularPustularAcne" checked>
            <%}%>
            <i for="acne_skin" class="fas fa-check"></i>
            <span class="new_checbox"></span>
            <label for="acne_skin">Trądzik grudkowo - kostkowy</label>
        </div>
        <div class="skin_diagnos_label">
            <%if(clients.skinDiagnoseAll.externallyDrySkin.value != true){%>
                <input type="checkbox" id="outside_dry_skin" value="<%=clients.skinDiagnoseAll.externallyDrySkin.value=true%>" name="externallyDrySkin">
                <%}else{%>
                <input type="checkbox" id="outside_dry_skin" value="<%=clients.skinDiagnoseAll.externallyDrySkin.value=true%>" name="externallyDrySkin" checked>
                <%}%>
            <i for="outside_dry_skin" class="fas fa-check"></i>
            <span class="new_checbox"></span>
            <label for="outside_dry_skin">Zewnętrznie przesuszona</label>
        </div>
    </div>

</div>
<div class="under_input">                     
    <label id="other"></label><br>
    <i class="fas fa-comment-dots"></i>
    <input type="TEXT" id="other" placeholder="inne..." onfocusout="hide_placeholder(this)" onfocus="show_placeholder(this)" name="other" value="<%=clients.skinDiagnoseAll.other%>" >
</div>

