                <input type="checkbox" id="dry_skin" name="drySkin"  value="<%=clients.drySkin=true%>" >
            <%}else{%>
                <input type="checkbox" id="dry_skin" name="drySkin"  value="<%=clients.drySkin=true%>"checked >
            <%}%>
