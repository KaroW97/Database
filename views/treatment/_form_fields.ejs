<form action="/treatment" method="POST">
    <label id="treatmentName"></label>
    <input type="text" id="treatmentName" placeholder="Nazwa zabiegu" value="<%=treatment.treatmentName%>" name="treatmentName">

    <label id="treatmentPrice"></label>
    <input type="number" id="treatmentPrice" placeholder="Cena zabiegu" value="<%=treatment.treatmentPrice%>" name="treatmentPrice">
    <button type="submit">Dodaj</button>
</form>
